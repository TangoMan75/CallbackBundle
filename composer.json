{
    "name": "tangoman/callback-bundle",
    "description": "Callback Symfony Twig Extension Bundle",
    "version": "2.1.1",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "Callback",
        "Twig",
        "Extension",
        "Service",
        "Symfony",
        "Bundle"
    ],
    "authors": [
        {
            "email": "matthias.morin@gmail.com",
            "name": "Matthias Morin"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "require": {
        "php":       ">=5.5.0",
        "twig/twig": "^1.35|^2.4.4"
    },
    "require-dev": {
        "symfony/routing":         "*",
        "symfony/http-foundation": "*",
        "symfony/phpunit-bridge":  "^3.4"
    },
    "autoload": {
        "psr-4": {
            "TangoMan\\CallbackBundle\\": ""
        }
    },
    "autoload-dev": {
        "classmap": [
            "Tests"
        ]
    },
    "extra": {
        "symfony-tests-dir": "Tests"
     },
    "minimum-stability": "dev",
    "prefer-stable": true
}